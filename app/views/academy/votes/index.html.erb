<h1>Listing academy_votes</h1>

<table>
  <thead>
    <tr>
      <th>Vote</th>
      <th>User</th>
      <th>Answer</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @academy_votes.each do |academy_vote| %>
      <tr>
        <td><%= academy_vote.vote %></td>
        <td><%= academy_vote.user_id %></td>
        <td><%= academy_vote.answer_id %></td>
        <td><%= link_to 'Show', academy_vote %></td>
        <td><%= link_to 'Edit', edit_academy_vote_path(academy_vote) %></td>
        <td><%= link_to 'Destroy', academy_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote', new_academy_vote_path %>

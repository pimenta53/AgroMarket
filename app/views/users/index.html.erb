<h1>User#index</h1>
<p>Find me in app/views/user/index.html.erb</p>
<table>
	<tr>
		<td>Nome</td>
		<td>Nickname</td>
		<td>Link</td>
	</tr>
	<% @user.each do |user| %>
	<tr>
		<td><%= user.name %></td>
		<td><%= user.email %></td>
		<td><%= link_to 'Ver', user_path(user) %></td>
	</tr>
	<% end %>
</table>
<%= link_to "Home",root_path%> | <%= link_to "Voltar",:back %>
